        <mapping lowerBound="0" upperBound="2" defaultValue="-2">
            <mapEntry mapKey="ChoiceA" mappedValue="1"/>
            <mapEntry mapKey="ChoiceB" mappedValue="1"/>
        </mapping>
    </responseDeclaration>
    <outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float">
        <defaultValue>
            <value>0</value>
        </defaultValue>
    </outcomeDeclaration>
    <itemBody>
        <p>
            <b>Question 9</b>
        </p>
        <br/>
        <choiceInteraction responseIdentifier="RESPONSE" shuffle="true" maxChoices="2">
            <prompt>Even though your homepage is in the correct folder, you still arenâ€™t able to view it in a web
                browser.<br/>What
                permissions need to be set so that your webpage can be viewed?
                <br/>
                <br/>
            </prompt>
            <simpleChoice identifier="ChoiceA" fixed="false">Read</simpleChoice>
            <simpleChoice identifier="ChoiceB" fixed="false">Execute</simpleChoice>
            <simpleChoice identifier="ChoiceC" fixed="false">Write</simpleChoice>
            <simpleChoice identifier="ChoiceD" fixed="false">Edit</simpleChoice>
        </choiceInteraction>
    </itemBody>
    <responseProcessing template="http://www.imsglobal.org/question/qti_v2p0/rptemplates/map_response"/>
</assessmentItem>

